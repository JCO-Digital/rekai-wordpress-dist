{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rekai/recommendations",
  "version": "0.1.0",
  "title": "Rek.ai Recommendations",
  "category": "widgets",
  "icon": "editor-ul",
  "description": "Recommendations block using Rek.ai.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "nrofhits": {
      "type": "string",
      "default": "10"
    },
    "headerText": {
      "type": "string",
      "default": ""
    },
    "renderstyle": {
      "type": "string",
      "default": ""
    },
    "userootpath": {
      "type": "boolean",
      "default": false
    },
    "currentLanguage": {
      "type": "boolean",
      "default": true
    },
    "addcontent": {
      "type": "boolean",
      "default": false
    },
    "subtree": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "rekai-wordpress",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
